/* Документация по Zenstack: https://zenstack.dev/ */
import "models/user"

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

plugin prisma {
    provider = '@core/prisma'
    output = '../schema.prisma'
}

plugin hooks {
  provider = '@zenstackhq/tanstack-query'
  output = "../hooks"
  target = 'react'
}